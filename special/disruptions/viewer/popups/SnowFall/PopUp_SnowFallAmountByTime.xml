<?xml version="1.0" ?>
<configuration>
	<title>Snow Fall Amount by Time</title>
    <description>
    	<![CDATA[
<B>Grid code</B>: {grid_code}		
<B>Label</B>: {label}
<B>From date</B>: {fromdate}
<B>To date</B>: {todate}
	]]>
    </description>    
    <fields>
        <field name="grid_code"/>		
        <field name="label"/>
        <field name="fromdate">
		    <format dateformat="shortDateShortTime" useutc="true"/>
        </field>
        <field name="todate">
		    <format dateformat="shortDateShortTime" useutc="true"/>
        </field>		
    </fields>
</configuration>