<?xml version="1.0" ?>
<configuration>
	<title>Inactive Perimeter </title>
    <description>
    	<![CDATA[
<B>Fire Name</B>: {FIRE_NAME}
<B>Acres</B>: {ACRES}
<B>Agency</B>: {AGENCY}
<B>Date</B>: {DATE_TIME}
	]]>
    </description>    
    <fields>
        <field name="ACRES">
		    <format precision="1"/>
		</field>	 
        <field name="AGENCY"/>		
        <field name="COMMENTS"/>
		<field name="YEAR_"/>
        <field name="ACTIVE"/>		
	    <field name="FIRE_NAME"/>		
        <field name="DATE_TIME">
		    <format dateformat="shortDateShortTime"/>
        </field>
        <field name="FIRE"/> 
    </fields>
</configuration>