<?xml version="1.0" ?>
<configuration>
	<title>{STORMNAME}</title>
    <description>
    	<![CDATA[
<B>Category</B>: {STORMTYPE}
<B>Date/Time</B>: {MONTH} {DAY}, {YEAR} {HHMM}
<B>Latitude</B>: {LAT}
<B>Longitude</B>: {LON}
<B>Intensity</B>: {INTENSITY} MPH
	]]>
    </description>    
    <fields>
        <field name="STORMTYPE"/> 
        <field name="STORMNAME"/> 
        <field name="YEAR">
        	<format usethousandsseparator="false"/>
        </field> 
        <field name="MONTH"/>
        <field name="DAY">
        	<format usethousandsseparator="false"/>
        </field> 
        <field name="HHMM"/> 
        <field name="LAT"/> 
        <field name="LON"/> 
        <field name="INTENSITY"/> 
    </fields>
</configuration>